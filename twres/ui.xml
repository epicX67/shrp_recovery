	<templates>
		<template name="c_tab_settings">

			<button style="c_tab_set">
				<placement x="25" y="153"/>
				<text>General</text>
				<action function="page">settings</action>
			</button>

			<button style="c_tab_set">
				<placement x="265" y="153"/>
