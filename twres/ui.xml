		</template>
		<template name="c_tab_backup_options">
			<button style="c_tab">
				<placement x="480" y="50"/>
				<text>Backup</text>
				<action function="page">backup</action>
			</button>
			<button style="c_tab">
				<conditions>
					<condition var1="tw_include_encrypted_backup" var2="1"/>
					<condition var1="tw_encrypt_backup" var2="0"/>
					<condition var1="tw_enable_adb_backup" op="!=" var2="1"/>
				</conditions>
				<placement x="760" y="50"/>
				<text>Encryption</text>
				<actions>
					<action function="set">tw_password_not_match=0</action>
					<action function="page">backupencryption</action>
				</actions>
			</button>
		</template>
		<template name="c_tab_encryption">
			<button style="c_tab">
				<placement x="530" y="50"/>
				<text>Backup</text>
