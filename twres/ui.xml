				</actions>
			</button>
		</template>
		<template name="c_lock">
			<background color="%semi_transparent%"/>
			<button>
				<placement x="0" y="400"/>
				<font resource="c_lock_clock" color="#ffffffff"/>
				<text>%tw_time%</text>
				<image resource="c_lock_bg"/>
			</button>
			<button>
				<placement x="238" y="1460"/>
				<image resource="c_on_tap_unlock"/>
				<action function="overlay"/>
			</button>
			<action>
				<touch key="power+voldown"/>
				<action function="screenshot"/>
			</action>
		</template>
		<template name="c_console">
			<background color="%c_white%"/>
			<console>
				<placement x="55" y="300" w="%c_console_width%" h="%c_console_height%"/>
			</console>
		</template>
