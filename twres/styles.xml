<?xml version="1.0"?>
<recovery>
	<styles>
		<style name="input">
			<background color="%c_white%"/>
			<cursor color="%c_grey%" hasfocus="1" width="%input_line_width%"/>
			<font resource="font_m" color="%c_grey%"/>
		</style>
		<style name="checkbox">
			<font resource="c_dashboard_text_sort" color="%c_black%"/>
			<image checked="checkbox_true" unchecked="checkbox_false"/>
		</style>
		<!--epicX_button/text_style_start-->
		<style name="c_text_dashboard">
			<font resource="c_dashboard_header" color="%c_black%"/>
		</style>
		<style name="c_text_dashboard2">
			<font resource="c_dashboard_header" color="%c_white%"/>
		</style>
		<style name="c_reboot_text">
			<font resource="c_dashboard_text" color="%c_black%"/>
		</style>
		<style name="c_main_text_header">
			<font resource="c_header" color="%c_black%"/>
		</style>
		<style name="c_file_menu">
			<font resource="c_menu" color="%c_black%"/>
		</style>
		<style name="c_main_text_header_file">
			<font resource="c_header" color="%c_white%"/>
		</style>
		<style name="c_main_text_header_failed">
			<font resource="c_header" color="#FF0000"/>
		</style>
		<style name="c_main_text_header2">
			<font resource="c_header" color="%c_white%"/>
		</style>
		<style name="c_main_text_header_passed">
			<font resource="c_header" color="#0083E6"/>
		</style>
		<style name="c_sub">
			<font resource="c_dashboard_text_sort" color="%c_grey%"/>
		</style>
		<style name="c_sub_failed">
			<font resource="c_dashboard_text_sort" color="#FF0000"/>
		</style>
		<style name="c_circle3">
			<highlight color="%highlight_color%"/>
			<font resource="c_menu" color="%c_circle_color_handler%"/>
			<image resource="c_circle3"/>
		</style>
		<style name="c_circle4">
			<highlight color="%highlight_color%"/>
			<font resource="c_menu" color="%c_circle_color_handler%"/>
			<image resource="c_circle4"/>
		</style>
		<style name="c_file_trans_bg_style">
			<highlight color="%highlight_color%"/>
			<font resource="c_dashboard_text_sort" color="%c_black%"/>
			<image resource="c_file_trans_bg"/>
		</style>
		<style name="c_queue">
			<highlight color="%highlight_color%"/>
			<font resource="font_l" color="%text_button_color%"/>
			<image resource="c_queue"/>
		</style>
		<style name="c_tab">
			<highlight color="%highlight_color%"/>
			<font resource="c_dashboard_header" color="%c_grey%"/>
			<image resource="c_tab_backup_bg"/>
		</style>
		<style name="c_tab_set">
			<highlight color="%highlight_color%"/>
			<font resource="c_dashboard_header" color="%c_black%"/>
			<image resource="c_tab_backup_bg"/>
		</style>
		<style name="c_th_dark_white">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#ffffffff"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_red">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#FF0000"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_pink">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#FF0095"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_rpink">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#FD0F60"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_blue">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#0088FF"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_green">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#92D050"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_orange">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#FFC000"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_yellow">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#BF9000"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_violate">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#9148C8"/>
			<image resource="c_circle2"/>
		</style>
		<style name="c_th_dark_cyan">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#00F2C4"/>
			<image resource="c_circle2"/>
		</style>
		
		
		<style name="c_th_white_black">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#242422"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_red">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#FF0000"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_pink">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#FF0095"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_rpink">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#FD0F60"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_blue">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#0088FF"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_green">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#119F51"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_orange">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#E67300"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_yellow">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#BF9000"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_violate">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#4F2270"/>
			<image resource="c_circle1"/>
		</style>
		<style name="c_th_white_cyan">
			<highlight color="%highlight_color%"/>
			<font resource="c_th_txt" color="#009D9F"/>
			<image resource="c_circle1"/>
		</style>
		<!--epicX_button/text_style_end-->

		<style name="fab">
			<highlight color="%highlight_color%"/>
			<font resource="font_m" color="%button_text_color%"/>
		</style>

		<style name="terminal">
			<background color="%c_white%"/>
			<fastscroll rectcolor="%c_black%" w="%fastscroll_w%" rectw="%fastscroll_rectw%" recth="%fastscroll_recth%"/>
			<font resource="font_s" spacing="3" color="%c_grey%"/>
		</style>
		<style name="fileselector">
			<background color="%c_white%"/>
			<fastscroll rectcolor="%c_black%" w="%fastscroll_w%" rectw="%fastscroll_rectw%" recth="%fastscroll_recth%" />
			<icon folder="folder" file="file" />
			<font resource="font_s" color="%c_black%" />
		</style>
		<style name="c_file_for_magisk">
			<background color="%c_white%"/>
			<fastscroll rectcolor="%c_black%" w="%fastscroll_w%" rectw="%fastscroll_rectw%" recth="%fastscroll_recth%" />
			<icon folder="c_modules" file="file" />
			<font resource="font_s" color="%c_black%" />
		</style>

		<style name="listbox">
			<background color="%c_white%"/>
			<fastscroll rectcolor="%c_black%" w="%fastscroll_w%" rectw="%fastscroll_rectw%" recth="%fastscroll_recth%" />
			<icon selected="radio_true" unselected="radio_false" />
			<font resource="font_s" color="%c_grey%"/>
		</style>
		<style name="console">
			<background color="%c_white%"/>
			<font resource="c_console_font" color="%c_black%"/>
		</style>

		<style name="scrolllist">
			<background color="%c_white%"/>
			<font resource="font_s" color="%c_black%"/>
			<fastscroll rectcolor="%c_black%" w="%fastscroll_w%" linew="%fastscroll_linew%" rectw="%fastscroll_rectw%" recth="%fastscroll_recth%"/>
		</style>

		<style name="partitionlist">
			<background color="%c_white%"/>
			<fastscroll rectcolor="%c_black%" w="%fastscroll_w%" linew="%fastscroll_linew%" rectw="%fastscroll_rectw%" recth="%fastscroll_recth%"/>
			<icon selected="checkbox_true" unselected="checkbox_false"/>
			<font resource="font_s" color="%c_grey%"/>
		</style>

		<style name="partitionlist_storage">
			<background color="%c_white%"/>
			<placement x="50" y="%c_hawk_base_second_header%" w="970" h="1500"/>
			<fastscroll rectcolor="%c_black%" w="%fastscroll_w%" linew="%fastscroll_linew%" rectw="%fastscroll_rectw%" recth="%fastscroll_recth%"/>
			<icon selected="radio_true" unselected="radio_false"/>
			<font resource="font_s" color="%c_grey%"/>
		</style>

		<style name="slidervalue">
			<resource handle="handle"/>
			<font resource="c_hider" color="%c_white%"/>
			<colors line="%c_black%"/>
			<dimensions lineh="%slidervalue_lineh%" linepadding="%slidervalue_padding%" sliderw="%slidervalue_sliderw%" sliderh="%slidervalue_sliderh%"/>
		</style>

		<style name="patternpassword">
			<size name="tw_gui_pattern_grid_size" default="3"/>
		</style>
	</styles>
</recovery>